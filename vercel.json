{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/watch",
      "dest": "video.py"
    },
    {
      "src": "/channel",
      "dest": "channel.py"
    },
    {
      "src": "/search",
      "dest": "search.py"
    },
    {
      "src": "/",
      "dest": "home.py"
    },
    {
      "src": "/home",
      "dest": "home1.py"
    }
  ]
}
